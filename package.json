{
  "name": "praxis-ai",
  "version": "1.0.0",
  "description": "The Multi-Agent System that Turns Raw Code into Industry-Ready Research",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn app.main:app --reload --port 8000",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "clean": "rimraf frontend/dist frontend/node_modules node_modules"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shemIA-Hack-Nation/praxis-ai.git"
  },
  "keywords": [
    "ai",
    "research",
    "multi-agent",
    "automation",
    "code-analysis"
  ],
  "author": "Hack Nation 3.0 Team",
  "license": "MIT"
}